{
  "name": "babel-plugin-parameter-decorator",
  "version": "1.0.5",
  "description": "Function parameter decorator transform plugin for babel v7, just like typescript.",
  "main": "lib",
  "engine": {
    "node": "*"
  },
  "scripts": {
    "build": "babel index.js -d lib",
    "test": "babel -x .ts,.js test/src/ -d test/lib && ava",
    "test-watch": "babel -x .ts,.js test/src/ -d test/lib && ava",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WarnerHooh/babel-plugin-parameter-decorator.git"
  },
  "author": "Warner",
  "license": "MIT",
  "keywords": [
    "babel",
    "babel-plugin",
    "function",
    "parameter",
    "decorators",
    "typescript"
  ],
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/generator": "^7.3.4",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/runtime": "^7.3.1",
    "ava": "^1.1.0",
    "typescript": "^3.3.3333"
  },
  "ava": {
    "files": [
      "./test/index.js"
    ]
  }
}
